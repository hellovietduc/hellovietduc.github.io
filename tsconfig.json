{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@helpers/*": ["helpers/*"],
      "@layouts/*": ["layouts/*"],
      "@metadata.json": ["metadata.json"]
    }
  },
  "include": ["src/**/*"]
}
