{
  "extends": [
    "airbnb-base",
    "@vietduc",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true,
    "jest": false
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "import/no-unresolved": [
      2,
      {
        "ignore": [
          "^@theme"
        ]
      }
    ],
    "import/prefer-default-export": "off"
  }
}
